{
    "name": "webmavens/triki-obfuscator",
    "description": "Laravel package for database obfuscation using Triki",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "WebMavens\\Triki\\": "src/"
        }
    },
    "authors": [
        {
            "name": "webmavens",
            "email": "info@webmavens.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "WebMavens\\Triki\\TrikiServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-uninstall-cmd": [
            "rm -rf vendor/webmavens/triki-obfuscator"
        ],
        "post-install-cmd": [
            "@copy-hooks"
        ],
        "post-update-cmd": [
            "@copy-hooks"
        ],
        "copy-hooks": [
            "bash -c 'cp git-hooks .git/hooks/commit-msg && chmod +x .git/hooks/commit-msg && echo \"\u2705 commit-msg hook installed.\"'"
        ]
    },
    "minimum-stability": "dev",
    "require": []
}